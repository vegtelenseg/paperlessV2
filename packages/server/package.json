{
  "name": "paperless",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "lint": "gulp lint",
    "build": "gulp",
    "prettier": "prettier --write \"./src/**/*.{tsx,ts,js}\"",
    "prettier:list": "prettier --list-different --write \"./{src}/**/*.{tsx,ts,js}\"",
    "knex": "cross-env ts-node node_modules/knex/bin/cli.js",
    "migrate": "yarn knex migrate:latest",
    "rollback": "yarn knex migrate:rollback",
    "seed": "KNEX_SEED=true yarn knex seed:run",
    "start": "cross-env nodemon | ./node_modules/.bin/bunyan"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/convict": "^4.2.1",
    "@types/express-graphql": "^0.6.2",
    "@types/graphql": "^14.0.3",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/graphql-relay": "^0.4.8",
    "@types/knex": "^0.15.0",
    "@types/lodash": "^4.14.118",
    "@types/nodemailer": "^4.6.5",
    "@types/passport": "^1.0.0",
    "@types/passport-strategy": "^0.2.35",
    "@types/pg": "^7.4.11",
    "@types/pg-escape": "^0.2.0",
    "babel": "^6.23.0",
    "bunyan": "^1.8.12",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "fakerator": "^0.3.0",
    "gulp": "^4.0.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "nodemon": "^1.17.5",
    "prettier": "^1.15.3",
    "random-date-generator": "^1.0.2",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "convict": "^4.4.1",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "fetch": "^1.1.0",
    "graphql": "^14.1.1",
    "graphql-iso-date": "^3.6.1",
    "jaeger-client": "^3.13.0",
    "join-monster": "^2.0.16",
    "knex": "^0.16.3",
    "objection": "^1.4.0",
    "opentracing": "^0.14.3",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.7.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": false
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      "*.spec.ts",
      "*.sql"
    ],
    "exec": "ts-node --files src/server/server.ts",
    "ext": "js,ts"
  }
}
